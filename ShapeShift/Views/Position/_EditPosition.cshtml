@model IEnumerable<ShapeShift.Models.Position>


    <table id ="EditPos"  class="table" data-url='@Url.Action("LoadAllPositions","AppUsers")''>

        <tr>

            <th hidden>

                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.title)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            @Html.HiddenFor(modelitem => item.UserId)
          
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.title)
                </td>
                <td>

                    <button id="EditPosition" data-url='@Url.Action("Edit", "Position", new { id = item.PositionId })'>Edit Position</button>
                    <button id="DeletePosition" data-url='@Url.Action("Delete", "Position", new { id = item.PositionId })'>Delete Position</button>

                </td>
            </tr>
            <tr>
                <td id="EditPositionField" class="hidden">
                    @Html.Partial("~/Views/Position/_EditIndividualPosition.cshtml", item)
                </td>
            </tr>
        }

    </table>
    <script>


    </script>